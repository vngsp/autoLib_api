generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model PackageManager {
  id Int @id @default(autoincrement())
  name String
  install String
  libs Lib[]
}
model Lib {
  id        Int     @id @default(autoincrement())
  name      String
  category  String   
  install   String   
  dev       String?  
  packageManagerId Int
  packageManager PackageManager @relation(fields: [packageManagerId], references: [id])
}